<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.deadwoodassambly.ui.assembly.AssemblyFragment"
        android:label="@string/assembly"
        tools:layout="@layout/fragment_assembly" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.deadwoodassambly.ui.map.MapsFragment"
        android:label="@string/map"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.deadwoodassambly.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_adder"
        android:name="com.example.deadwoodassambly.ui.adder.AdderFragment"
        android:label="@string/navadder"
        tools:layout="@layout/fragment_adder" />
</navigation>